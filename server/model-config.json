    "dataSource": "db",
    "public": false
  },
  "user": {
    "dataSource": "db",
    "public": true,
    "relations": {
      "accessTokens": {
        "type": "hasMany",
        "model": "AccessToken",
        "foreignKey": "userId",
        "options": {
          "disableInclude": true
        }
      }
    }
  }
}
